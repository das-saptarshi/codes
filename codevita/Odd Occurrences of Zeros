def oddOccurrenceOfZeors(nums):
    result = []
    for num in nums:
        results.append(str(num).count('0') % 2 == 1)
    return result

N = int(input())
nums = list(map(int, input().split()))
result = oddOccurrenceOfZeros(nums)
print(*result)
